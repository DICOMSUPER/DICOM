  "scripts": {
    "start:dev": "nx run-many -t serve --all --inspect=false",
    "dev": "nx run-many -t serve --all --inspect=false",
    "// ===== DOCKER COMPOSE COMMANDS (RECOMMENDED) =====": "",
    "docker:build:local": "cross-env TAG=local docker-compose build",
    "docker:build:latest": "cross-env TAG=latest docker-compose build",
    "docker:up:local": "cross-env TAG=local docker-compose up -d",
    "docker:up:latest": "cross-env TAG=latest docker-compose up -d",
    "docker:up:local:build": "cross-env TAG=local docker-compose up -d --build",
    "docker:up:latest:build": "cross-env TAG=latest docker-compose up -d --build",
    "docker:down": "docker-compose down",
    "docker:logs": "docker-compose logs -f",
    "docker:logs:api": "docker-compose logs -f api-gateway",
    "docker:logs:user": "docker-compose logs -f user-service",
    "docker:logs:patient": "docker-compose logs -f patient-service",
    "docker:logs:imaging": "docker-compose logs -f imaging-service",
    "docker:logs:system": "docker-compose logs -f system-service",
    "docker:ps": "docker-compose ps",
    "docker:restart": "docker-compose restart",
    "// ===== LEGACY INDIVIDUAL DOCKER COMMANDS =====": "",
    "build-docker:local:api-gateway": "docker build -t dicom/api-gateway:local -f apps/api-gateway/Dockerfile .",
    "build-docker:local:user-service": "docker build -t dicom/user-service:local -f apps/user-service/Dockerfile .",
    "build-docker:local:patient-service": "docker build -t dicom/patient-service:local -f apps/patient-service/Dockerfile .",
    "build-docker:local:imaging-service": "docker build -t dicom/imaging-service:local -f apps/imaging-service/Dockerfile .",
    "build-docker:local:system-service": "docker build -t dicom/system-service:local -f apps/system-service/Dockerfile .",
    "build-docker:local:ws-gateway": "docker build -t dicom/ws-gateway:local -f apps/ws-gateway/Dockerfile .",
    "build-docker:local:all": "npm run build-docker:local:api-gateway && npm run build-docker:local:user-service && npm run build-docker:local:patient-service && npm run build-docker:local:imaging-service && npm run build-docker:local:system-service",
    "build-docker:latest:api-gateway": "docker build -t dicom/api-gateway:latest -f apps/api-gateway/Dockerfile .",
    "build-docker:latest:user-service": "docker build -t dicom/user-service:latest -f apps/user-service/Dockerfile .",
    "build-docker:latest:patient-service": "docker build -t dicom/patient-service:latest -f apps/patient-service/Dockerfile .",
    "build-docker:latest:imaging-service": "docker build -t dicom/imaging-service:latest -f apps/imaging-service/Dockerfile .",
    "build-docker:latest:system-service": "docker build -t dicom/system-service:latest -f apps/system-service/Dockerfile .",
    "build-docker:latest:all": "npm run build-docker:latest:api-gateway && npm run build-docker:latest:user-service && npm run build-docker:latest:patient-service && npm run build-docker:latest:imaging-service && npm run build-docker:latest:system-service",
    "run-docker:local:api-gateway": "docker run --rm -p 5000:5000 --env-file apps/api-gateway/.env dicom/api-gateway:local",
    "run-docker:local:user-service": "docker run --rm -p 5002:5002 --env-file apps/user-service/.env dicom/user-service:local",
    "run-docker:local:patient-service": "docker run --rm -p 5004:5004 --env-file apps/patient-service/.env dicom/patient-service:local",
    "run-docker:local:system-service": "docker run --rm -p 5005:5005 --env-file apps/system-service/.env dicom/system-service:local",
    "run-docker:local:imaging-service": "docker run --rm -p 5003:5003 --env-file apps/imaging-service/.env dicom/imaging-service:local",
    "run-docker:latest:api-gateway": "docker run --rm -p 5000:5000 --env-file apps/api-gateway/.env dicom/api-gateway:latest",
    "run-docker:latest:user-service": "docker run --rm -p 5002:5002 --env-file apps/user-service/.env dicom/user-service:latest",
    "run-docker:latest:patient-service": "docker run --rm -p 5004:5004 --env-file apps/patient-service/.env dicom/patient-service:latest",
    "run-docker:latest:system-service": "docker run --rm -p 5005:5005 --env-file apps/system-service/.env dicom/system-service:latest",
    "run-docker:latest:imaging-service": "docker run --rm -p 5003:5003 --env-file apps/imaging-service/.env dicom/imaging-service:latest"
  },
