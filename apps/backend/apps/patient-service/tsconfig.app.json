{
  "extends": "../../tsconfig.base.json",
  "compilerOptions": {
    "outDir": "dist",
    "types": ["node"],
    "rootDir": "src",
    "tsBuildInfoFile": "dist/tsconfig.app.tsbuildinfo",
    "experimentalDecorators": true,
    "emitDecoratorMetadata": true,
    "target": "es2021"
  },
  "include": [
    "src/**/*.ts",
    "../../libs/shared-domain/src/lib/entities/patients/diagnoses-reports.entity.ts",
    "../../libs/shared-domain/src/lib/entities/patients/patient-conditions.entity.ts",
    "../../libs/shared-domain/src/lib/entities/patients/patient-encounters.entity.ts",
    "../../libs/shared-domain/src/lib/entities/patients/patients.entity.ts",
    "../../libs/shared-domain/src/lib/dto/patient-service/queue-assignment/update-queue-assignment.dto.ts",
    "../../libs/shared-domain/src/lib/dto/patient-service/queue-assignment/create-queue-assignment.dto.ts"
  ],
  "exclude": [
    "out-tsc",
    "dist",
    "jest.config.ts",
    "src/**/*.spec.ts",
    "src/**/*.test.ts",
    "eslint.config.js",
    "eslint.config.cjs",
    "eslint.config.mjs"
  ],
  "references": [
    {
      "path": "../../libs/shared-interfaces/tsconfig.lib.json"
    },
    {
      "path": "../../libs/shared-client/tsconfig.lib.json"
    },
    {
      "path": "../../libs/shared-decorators/tsconfig.lib.json"
    },
    {
      "path": "../../libs/shared-enums/tsconfig.lib.json"
    },
    {
      "path": "../../libs/shared-domain/tsconfig.lib.json"
    },
    {
      "path": "../../libs/redis/tsconfig.lib.json"
    },
    {
      "path": "../../libs/database/tsconfig.lib.json"
    },
    {
      "path": "../../libs/shared-utils/tsconfig.lib.json"
    }
  ]
}
